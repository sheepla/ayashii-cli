#!/bin/bash

set -o errexit
set -o nounset

readonly AUTHOR_URL='https://github.com/sheepla'
readonly REPO_URL="${AUTHOR_URL}/ayashii-cli"

function _help() {
    echo -e "
ayashii -- AYASHII NIHONGO CLI: Convert from normal Japanese text to AYASHII NIHONGO.

USAGE
    ayashii TEXT        # from arguments
    echo TEXT | ayashii # from stdin
    ayashii --help      # Show this help.

EXAMPLE
    $ ayashii ã“ã‚Œã¯æ­£ã—ã„æ—¥æœ¬èªã§ã™ï¼
    ã“ã‚Œã¯æ­£ãƒ¬ã„æ—¥æœ¬è¯­ã¦ãšï¼
    $ echo ã“ã‚Œã¯æ­£ã—ã„æ—¥æœ¬èªã§ã™ï¼ | ayashii
    ã“ã‚Œã¯æ­£ãƒ¬ã„æ—¥æœ¬è¯­ã¦ãšï¼

REPO
    Repository:      ${REPO_URL}
    Author:          sheepla (${AUTHOR_URL})
    Licence:         MIT
"
}

function _err() {
    echo -e "[\e[31m ERR \e[m] ${*}"
}

function _to_ayashii() {
    text="${*}"
    echo "$text" | sed '
    # source: https://github.com/Submarinonline/cjp.js/tree/master/dict

    # emoji.json
    s/ğŸ‡¦ğŸ‡º/ğŸ‡¨ğŸ‡³/g
    s/ğŸ‡¯ğŸ‡µ/ğŸ‡¨ğŸ‡³/g
    s/ğŸ‡°ğŸ‡·/ğŸ‡¨ğŸ‡³/g
    s/ğŸŒ/ğŸ‡¨ğŸ‡³/g

    # common.json
    s/(?<!ãŠã‚„ã™ã¿|ã”ã‚ã‚“)ãªã•ã„/ï¼ˆã—ãªã•ã„ï¼‰/g
    s/(?<!ã¯ã„)ã¯ã„(?=ã€|ã€‚|ã€|$)/ã°ã„/g
    s/(?<=[0-9]|å|ç™¾|åƒ|ä¸‡|å„„|å…†)å††/æ—¥å…ƒ/g
    s/(?<=ä¼˜ç§€ã®)å“/è‰¯å“/g
    s/(?<=ä¼˜ç§€ã®)å•†å“/è‰¯å“/g
    s/(?<=ä¼˜ç§€ã®)è£½å“/è‰¯å“/g
    s/(?<=å„ªç§€|ä¼˜ç§€)ã®/ã®/g
    s/(?<=å„ªç§€|æœ€é«˜)ãª/ã®/g
    s/(?<=æš‘)ã„(?=æ—¥)/ã®/g
    s/\(ã—ãªã•ã„\)|ï¼ˆã—ãªã•ã„ï¼‰/ï¼ˆã—ãªã•ã„ï¼‰/g
    s/ã„ã‘ãªã„/ãƒã’ãªã„/g
    s/ãŠã‚ã§ã¨ã†(?=ã€‚|$)/æ­å–œï¼/g
    s/ãŠã‚ã§ã¨ã†(?=ï¼|!)/æ­å–œ/g
    s/ã‹ã—ã“ã„(?!ã€‚|$)/ä¼˜ç§€ã®/g
    s/ãã ã•ã„/ããŸã¡ã„/g
    s/ã“ã‚“ã«ã¡ã¯/ã”ã‚“ã«ã•ã¯/g
    s/ã“ã‚“ã°ã‚“ã¯/ã”ã‚“ã¯ã‚“ã°/g
    s/ã•ã‚ˆã†ãªã‚‰/ã¡ã‚ˆãƒ©ãªã‚‰/g
